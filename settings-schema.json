{
  "head": {
    "type": "header"
  },
  "generalSettings": {
    "type": "header"
  },
  "linuxWPEPath": {
    "type": "entry",
    "default": "~/linux-wallpaperengine/build"
  },
  "screen": {
    "type": "entry",
    "default": "DisplayPort-2"
  },
  "wallpaperDir": {
    "type": "entry",
    "default": "~/.steam/debian-installation/steamapps/workshop/content/431960"
  },
  "logFile": {
    "type": "entry",
    "default": "/var/log/wallpaper-shuffle.log"
  },
  "volumeLevel": {
    "type": "spinbutton",
    "default": 50,
    "min": 0,
    "max": 100,
    "units": "%",
    "step": 1
  },
  "shuffleInterval": {
    "type": "spinbutton",
    "default": 10,
    "min": 1,
    "max": 120,
    "units": "minutes",
    "step": 1
  },
  "disableMouse": {
    "type": "checkbox",
    "default": false
  },
  "currentStatus": {
    "type": "header"
  },
  "currentWallpaper": {
    "type": "entry",
    "default": "",
    "editable": false
  },
  "previousWallpaper": {
    "type": "entry",
    "default": "",
    "editable": false
  },
  "queueLength": {
    "type": "entry",
    "default": 396,
    "editable": false
  },
  "queue": {
    "type": "array",
    "default": [],
    "editable": false
  },
  "currentIndex": {
    "type": "label",
    "default": 11,
    "editable": false
  },
  "shuffleStatus": {
    "type": "label",
    "default": "Stopped",
    "editable": false
  },
  "help": {
    "type": "button",
    "description": "Open GitHub Page",
    "callback": "openHelp",
    "tooltip": "https://github.com/abcdqfr/wallpaper-shuffle"
  },
  "wallpaperList": {
    "default": []
  }
}
