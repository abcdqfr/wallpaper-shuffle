{
  "head": {
    "type": "header"
  },
  "general-settings": {
    "title": "General Settings",
    "type": "header"
  },
  "linux-wpe-path": {
    "type": "entry",
    "default": "~/linux-wallpaperengine/build"
  },
  "screen": {
    "type": "entry",
    "default": "DisplayPort-2"
  },
  "wallpaper-dir": {
    "type": "entry",
    "default": "~/.steam/debian-installation/steamapps/workshop/content/431960"
  },
  "log-file": {
    "type": "entry",
    "default": "/var/log/wallpaper-shuffle.log"
  },
  "volume-level": {
    "type": "slider",
    "default": 50,
    "min": 0,
    "max": 100,
    "units": "percent",
    "step": 1
  },
  "shuffle-interval": {
    "type": "spinbutton",
    "default": 10,
    "min": 1,
    "max": 120,
    "units": "minutes",
    "step": 1
  },
  "disable-mouse": {
    "type": "checkbox",
    "default": false
  },
  "current-status": {
    "type": "header"
  },
  "current-wallpaper": {
    "type": "entry",
    "default": ""
  },
  "previous-wallpaper": {
    "type": "entry",
    "default": ""
  },
  "queue-length": {
    "type": "entry",
    "default": 0
  },
  "queue": {
    "type": "list",
    "default": []
  },
  "current-index": {
    "default": 0
  },
  "shuffle-status": {
    "default": "Stopped"
  }
}
